{
  "name": "github-stats",
  "version": "1.0.0",
  "repository": "git@github.com:reizt/github-stats.git",
  "author": "reizt <reizt.dev@gmail.com>",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "node --no-warnings esbuild.mjs vercel",
    "dev": "nodemon --exec \"run-s dev:build dev:start\" --watch src --ext ts --delay 0.2",
    "dev:build": "node --no-warnings esbuild.mjs launch",
    "dev:start": "node --enable-source-maps .build/launch.js",
    "lint": "run-s prettier:l eslint:l tsc:l",
    "format": "run-s prettier:f eslint:f",
    "eslint:l": "eslint 'src/**'",
    "eslint:f": "eslint --fix 'src/**'",
    "prettier:l": "prettier --check --log-level silent src",
    "prettier:f": "prettier --write --log-level silent src",
    "tsc:l": "tsc --noEmit --outDir .build",
    "tsc:w": "tsc --noEmit --outDir .build --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage && open coverage/index.html"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.10.9",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
