{
	"version": 2,
	"installCommand": "yarn install",
	"buildCommand": "yarn build",
	"devCommand": "yarn start",
	"outputDirectory": ".",
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api"
		},
		{
			"source": "/",
			"destination": "https://github.com/reizt/github-stats"
		}
	]
}
